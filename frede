from sense_hat import SenseHat
sense = SenseHat()

g = (0, 255, 0)
b = (0, 0, 255)
dg = (0, 20, 0)
y = (255, 255, 0)
bob_pixels = [
    dg, dg, dg, dg, b, y, y, y,
    dg, dg, dg, b, b, b, y, y,
    dg, dg, b, b, b, b, b, y,
    dg, dg, b, b, b, b, b, b,
    dg, dg, b, b, b, b, b, b,
    dg, dg, g, g, g, g, g, g,
    g, g, g, g, g, g, g, g,
    g, g, g, g, g, g, g, g
  ]
  
sense.set_pixels(bob_pixels)
